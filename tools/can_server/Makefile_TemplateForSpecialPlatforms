#############################################################################
# Makefile for building: can_server_special_platform
# Project:               can_server
#############################################################################

# set this variable before (!) include Makefile to get correct CC, LINK, .. variables
COMPILER_BINARY_PRE :=<cross compile directory>/<cross compile prefix>

-include Makefile

# platform specific stuff
SOURCES_MSQ_SPECIAL := \
	src/can_server_common.cpp \
	$(SOURCES_MSQ) \
	src/devices/can_device_special.cpp

SOURCES_ALL_MSQ := $(SOURCES_ALL_MSQ) src/devices/can_device_special.cpp


$(call PRE_EXEC,can_server_special_platform): $(call OBJECTS,msq,$(SOURCES_MSQ_SPECIAL))
	$(LINK) -o $@ $^ $(CLFLAGS) -lrt
	$(STRIP) $@
