<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
  <!-- ######################################################################################################################### -->
  <!-- ### That XML scheme is specified for use with basic vt2iso and VT Revision 2 or earlier (support for block font etc.) ### -->
  <!-- ### If attributes and elements are commented out, they belong to VT Revision 3 or later ### -->
  <!-- ######################################################################################################################### -->
  
  <!-- ### Attribute-Values as simpleTypes ### -->
  <!-- ### Numeric Attribute-Values ### -->
  <xs:simpleType name="attrObjectId">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="65534"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrUnspecifiedObjectId">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:enumeration value="65535"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrNumberOfDecimals">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="7"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrPercentage">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="100"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attr0to250">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="250"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attr0to254">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="254"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attr0to255">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="255"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attr1to255">
    <xs:restriction base="xs:positiveInteger">
      <xs:maxInclusive value="255"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attr3to255">
    <xs:restriction base="xs:positiveInteger">
      <xs:minInclusive value="3"/>
      <xs:maxInclusive value="255"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrPositiveSigned16BitVal">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="32767"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrSigned16BitVal">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="-32767"/>
      <xs:maxInclusive value="32767"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrPositive16BitVal">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="65535"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrPositive32BitVal">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="4294967296"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrSigned32BitVal">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="-2147483648"/>
      <xs:maxInclusive value="2147483648"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrAngle">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="180"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrAttributeID">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:maxInclusive value="15"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrPositiveSignedInt">
    <xs:restriction base="xs:short">
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrViewportZoom">
    <xs:restriction base="xs:short">
      <xs:minInclusive value="-32"/>
      <xs:minInclusive value="32"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- ### Alphanumeric Attribute-Values ### -->
  <xs:simpleType name="attrObjectNameOrId">
    <xsd:union memberTypes="xs:string attrObjectId"/>
  </xs:simpleType>
  
  <xs:simpleType name="attrObjectNameOrIdUnspecified">
    <xsd:union memberTypes="xs:string attrObjectId attrUnspecifiedObjectId"/>
  </xs:simpleType>
  
  <xs:simpleType name="attrColour">
    <xs:restriction base="xs:string">
      <xs:pattern value="black|white|green|teal|maroon|purple|olive|silver|grey|blue|lime|cyan|red|magenta|yellow|navy|[1-9]?[0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrFontSize">
    <xs:restriction base="xs:string">
      <xs:pattern value="6x8|8x8|8x12|12x16|16x16|16x24|24x32|32x32|32x48|48x64|64x64|64x96|96x128|128x128|128x192|1[0-4]|[0-9]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrFontType">
    <xs:restriction base="xs:string">
      <xs:pattern value="0|1|2|255|latin1|latin9|latin5|proprietary"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrLineDirection">
    <xs:restriction base="xs:string">
      <xs:pattern value="toplefttobottomright|bottomlefttotopright|[0-1]"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrLineSuppression">
    <xs:restriction base="xs:string">
      <xs:pattern value="((top|right|bottom|left)?(\+(top|right|bottom|left))*)|(1[0-5]|[0-9])"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrEllipseType">
    <xs:restriction base="xs:string">
      <xs:pattern value="closed|open|closedsegment|closedsection|[0-3]"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrPolygonType">
    <xs:restriction base="xs:string">
      <xs:pattern value="convex|nonconvex|complex|open|[0-3]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrFillType">
    <xs:restriction base="xs:string">
      <xs:pattern value="nofill|linecolour|fillcolour|pattern|[0-3]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrAcousticSignal">
    <xs:restriction base="xs:string">
      <xs:pattern value="high|medium|low|none|[0-3]"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrPriority">
    <xs:restriction base="xs:string">
      <xs:pattern value="high|medium|low|[0-2]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrAuxFunctionType">
    <xs:restriction base="xs:string">
      <xs:pattern value="latchingboolean|analog|nonlatchingboolean|[0-2]"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- ### Textual Attribute-Values ### -->
  <xs:simpleType name="attrFontStyle">
    <xs:restriction base="xs:string">
      <xs:pattern value="normal|(bold|crossed|underlined|italic|inverted|flashinginverted|flashinghidden)?(\+(bold|crossed|underlined|italic|inverted|flashinginverted|flashinghidden))*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrFormatNumber">
    <xs:restriction base="xs:string">
      <xs:enumeration value="fixed"/>
      <xs:enumeration value="exponential"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrHorizontalJustification">
    <xs:restriction base="xs:string">
      <xs:enumeration value="left"/>
      <xs:enumeration value="middle"/>
      <xs:enumeration value="right"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrRle">
    <xs:restriction base="xs:string">
      <xs:pattern value="auto|(\+?(1bit|4bit|8bit))*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrFormatPictureGraphicFixedBitmap">
    <xs:restriction base="xs:string">
      <xs:pattern value="1bit|4bit|8bit"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrValidationType">
    <xs:restriction base="xs:string">
      <xs:pattern value="(in)?valid_characters"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrYesNo">
    <xs:restriction base="xs:string">
      <xs:enumeration value="yes"/>
      <xs:enumeration value="no"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrTrueFalse">
    <xs:restriction base="xs:string">
      <xs:enumeration value="true"/>
      <xs:enumeration value="false"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrOnOff">
    <xs:restriction base="xs:string">
      <xs:enumeration value="on"/>
      <xs:enumeration value="off"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrShowHide">
    <xs:restriction base="xs:string">
      <xs:enumeration value="show"/>
      <xs:enumeration value="hide"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrEnableDisable">
    <xs:restriction base="xs:string">
      <xs:enumeration value="enable"/>
      <xs:enumeration value="disable"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrLanguageCode">
    <xs:restriction base="xs:string">
      <xs:pattern value="(\*)|((aa|ab|af|am|ar|as|ay|az|ba|be|bg|bh|bi|bn|bo|br|ca|co|cs|cy|da|de|dz|el|en|eo|es|et|eu|fa|fi|fj|fo|fr|fy|ga|gd|gl|gn|gu|ha|hi|hr|hu|hy|ia|ie|ik|in|is|it|iw|ja|ji|jw|ka|kk|kl|km|kn|ko|ks|ku|ky|la|ln|lo|lt|lv|mg|mi|mk|ml|mn|mo|mr|ms|mt|my|na|ne|nl|no|oc|om|or|pa|pl|ps|pt|qu|rm|rn|ro|ru|rw|sa|sd|sg|sh|si|sk|sl|sm|sn|so|sq|sr|ss|st|su|sv|sw|ta|te|tg|th|ti|tk|tl|tn|to|tr|ts|tt|tw|uk|ur|uz|vi|vo|wo|xh|yo|zh|zu)(\+)?)*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrMaskType">
    <xs:restriction base="xs:string">
      <xs:pattern value="undefined|data|alarm"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrOptionsButton">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((latchable|latched|suppress_border|transparent_background|disabled|no_border)(\+(latchable|latched|suppress_border|transparent_background|disabled|no_border))*)?"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrOptionsString">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((transparent|autowrap|wrap_on_hyphen)(\+(transparent|autowrap|blankzero|wrap_on_hyphen))*)?"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrOptionsNumber">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((transparent|leadingzeros|blankzero|truncate)(\+(transparent|leadingzeros|blankzero|truncate))*)?"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrInputOptions">
    <xs:restriction base="xs:string">
      <xs:pattern value="((enable|live_editing)(\+(enable|live_editing))*)?"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="attrOptionsPictureGraphic">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|(rle|rle1|rle4|rle8|opaque|normal|transparent|flashing)?(\+(rle|rle1|rle4|rle8|opaque|normal|transparent|flashing))*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrOptionsMeter">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((arc|border|ticks|clockwise)(\+(arc|border|ticks|clockwise))*)?"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrOptionsLinearBarGraph">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((border|targetline|ticks|nofill|horizontal|growpositive)(\+(border|targetline|ticks|nofill|horizontal|growpositive))*)?"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrOptionsArchedBarGraph">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((border|targetline|nofill|clockwise)(\+(border|targetline|nofill|clockwise))*)?"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrObjectGraphicsContext">
    <xs:restriction base="xs:string">
      <xs:pattern value="none|((transparent|uselinefontfillcolourfordraw)(\+(transparent|uselinefontfillcolourfordraw))*)?"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="attrEventType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="on_activate"/>
      <xs:enumeration value="on_deactivate"/>
      <xs:enumeration value="on_show"/>
      <xs:enumeration value="on_hide"/>
      <xs:enumeration value="on_enable"/>
      <xs:enumeration value="on_disable"/>
      <xs:enumeration value="on_change_active_mask"/>
      <xs:enumeration value="on_change_soft_key_mask"/>
      <xs:enumeration value="on_change_attribute"/>
      <xs:enumeration value="on_change_background_colour"/>
      <xs:enumeration value="on_change_font_attributes"/>
      <xs:enumeration value="on_change_line_attributes"/>
      <xs:enumeration value="on_change_fill_attributes"/>
      <xs:enumeration value="on_change_child_location"/>
      <xs:enumeration value="on_change_size"/>
      <xs:enumeration value="on_change_value"/>
      <xs:enumeration value="on_change_priority"/>
      <xs:enumeration value="on_change_end_point"/>
      <xs:enumeration value="on_input_field_selection"/>
      <xs:enumeration value="on_input_field_deselection"/>
      <xs:enumeration value="on_ESC"/>
      <xs:enumeration value="on_entry_of_value"/>
      <xs:enumeration value="on_entry_of_new_value"/>
      <xs:enumeration value="on_key_press"/>
      <xs:enumeration value="on_key_release"/>
      <xs:enumeration value="on_change_child_position"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- ### Following object types are used for the macro only ### -->
  <xs:complexType name="elemCmdHideShow">
    <xs:attribute use="required" name="object_id" type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="hide_show" type="attrTrueFalse"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdEnableDisable">
    <xs:attribute use="required" name="object_id" type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="enable_disable" type="attrTrueFalse"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdSelectInput">
    <xs:attribute use="required" name="object_id" type="attrObjectNameOrIdUnspecified"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdControlAudioDevice">
    <xs:attribute use="required" name="object_id"             type="attrObjectNameOrId"/>
    <xs:attribute use="optional" name="number_of_repetitions" type="attr1to255"/>
    <xs:attribute use="optional" name="frequency"             type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="on_time_duration"      type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="off_time_duration"     type="attrPositive16BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdSetAudioVolume">
    <xs:attribute use="required" name="object_id"  type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="percentage" type="attrPercentage"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeChildLocation">
    <xs:attribute use="required" name="parent_object_id" type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="object_id"        type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="x_pos_change"     type="attr0to255"/>
    <xs:attribute use="required" name="y_pos_change"     type="attr0to255"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeChildPosition">
    <xs:attribute use="required" name="parent_object_id" type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="object_id"        type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="x_pos"            type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="y_pos"            type="attrPositive16BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeSize">
    <xs:attribute use="required" name="object_id"  type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_width"  type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="new_height" type="attrPositive16BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeBackgroudColour">
    <xs:attribute use="required" name="object_id"             type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_background_colour" type="attrColour"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeNumericValue">
    <xs:attribute use="required" name="object_id" type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_value" type="attrPositive32BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeStringValue">
    <xs:attribute use="required" name="object_id"       type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="bytes_in_string" type="xs:attrPositive16BitVal"/>
    <xs:attribute use="required" name="new_value"       type="xs:string"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeEndPoint">
    <xs:attribute use="required" name="object_id"      type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_width"      type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="new_height"     type="attrPositive16BitVal"/>
    <xs:attribute use="required" name="line_direction" type="attrLineDirection"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeFontAttributes">
    <xs:attribute use="required" name="object_id"   type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="font_colour" type="attrColour"/>
    <xs:attribute use="required" name="font_size"   type="attrFontSize"/>
    <xs:attribute use="required" name="font_type"   type="attrFontType"/>
    <xs:attribute use="required" name="font_style"  type="attrFontStyle"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeLineAttributes">
    <xs:attribute use="required" name="object_id"   type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="line_colour" type="attrColour"/>
    <xs:attribute use="required" name="line_width"  type="attr0to255"/>
    <xs:attribute use="required" name="line_art"    type="attrPositive16BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeFillAttributes">
    <xs:attribute use="required" name="object_id"    type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="fill_type"    type="attrFillType"/>
    <xs:attribute use="required" name="fill_colour"  type="attrColour"/>
    <xs:attribute use="required" name="fill_pattern" type="attrObjectNameOrIdUnspecified"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeActiveMask">
    <xs:attribute use="required" name="object_id"                 type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="working_set_object_id"     type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_active_mask_object_id" type="attrObjectNameOrId"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeSoftKeyMask">
    <xs:attribute use="required" name="object_id"                  type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="mask_type"                  type="attrMaskType"/>
    <xs:attribute use="required" name="mask_object_id"             type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_softkey_mask_object_id" type="attrObjectNameOrId"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeAttribute">
    <xs:attribute use="required" name="object_id"    type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="attribute_id" type="attrAttributeId"/>
    <xs:attribute use="required" name="new_value"    type="attrSigned32BitVal"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangePriority">
    <xs:attribute use="required" name="object_id"    type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="new_priority" type="attrPriority"/>
  </xs:complexType>
  
  <xs:complexType name="elemCmdChangeListItem">
    <xs:attribute use="required" name="object_id"     type="attrObjectNameOrId"/>
    <xs:attribute use="required" name="list_index"    type="attr0to254"/>
    <xs:attribute use="required" name="new_object_id" type="attrObjectNameOrIdUnspecified"/>
  </xs:complexType>
  

  <!-- ######################################## -->
  <!-- ### All Object-Types as complexTypes ### -->
  <xs:complexType name="elemWorkingset">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <!--<xs:element name="container"       type="elemContainer"/>-->
        <xs:element name="outputstring"    type="elemOutputString"/>
        <xs:element name="outputnumber"    type="elemOutputNumber"/>
        <!--<xs:element name="outputlist"      type="elemOutputList"/>-->
        <xs:element name="line"            type="elemLine"/>
        <xs:element name="rectangle"       type="elemRectangle"/>
        <xs:element name="ellipse"         type="elemEllipse"/>
        <xs:element name="polygon"         type="elemPolygon"/>
        <!--<xs:element name="meter"           type="elemMeter"/>
        <xs:element name="linearbargraph"  type="elemLinearBargraph"/>
        <xs:element name="archedbargraph"  type="elemArchedBargraph"/>-->
        <xs:element name="picturegraphic"  type="elemPictureGraphic"/>
        <!--<xs:element name="graphicscontext" type="elemGraphicsContext"/>
        <xs:element name="objectpointer"   type="elemObjectPointer"/>-->
        <xs:element name="language"        type="elemLanguage"/>
        <xs:element name="include_macro"   type="elemIncludeMacro"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute use="optional" name="id"                type="attrObjectId"/>
    <xs:attribute use="optional" name="name"              type="xs:string"/>
    <xs:attribute use="required" name="background_colour" type="attrColour"/>
    <xs:attribute use="required" name="selectable"        type="attrYesNo"/>
    <xs:attribute use="required" name="active_mask"       type="attrObjectNameOrId"/>
  </xs:complexType>

  <xs:complexType name="elemDatamask">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="container"        type="elemContainer"/>
        <xs:element name="button"           type="elemButton"/>
        <xs:element name="inputboolean"     type="elemInputBoolean"/>
        <xs:element name="inputstring"      type="elemInputString"/>
        <xs:element name="inputnumber"      type="elemInputNumber"/>
        <xs:element name="inputlist"        type="elemInputList"/>
        <xs:element name="outputstring"     type="elemOutputString"/>
        <xs:element name="outputnumber"     type="elemOutputNumber"/>
        <!--<xs:element name="outputlist"       type="elemOutputList"/>-->
        <xs:element name="line"             type="elemLine"/>
        <xs:element name="rectangle"        type="elemRectangle"/>
        <xs:element name="ellipse"          type="elemEllipse"/>
        <xs:element name="polygon"          type="elemPolygon"/>
        <xs:element name="meter"            type="elemMeter"/>
        <xs:element name="linearbargraph"   type="elemLinearBargraph"/>
        <xs:element name="archedbargraph"   type="elemArchedBargraph"/>
        <xs:element name="picturegraphic"   type="elemPictureGraphic"/>
        <!--<xs:element name="graphicscontext"  type="elemGraphicsContext"/>-->
        <xs:element name="objectpointer"    type="elemObjectPointer"/>
        <xs:element name="language"         type="elemLanguage"/>
        <!--<xs:element name="auxobjectpointer" type="elemAuxObjectPointer"/>-->
        <xs:element name="include_object"   type="elemIncludeObject"/>
        <xs:element name="include_macro"    type="elemIncludeMacro"/>
      </xs:choice>
