<?xml version="1.0" encoding="iso-8859-1"?>

<objectpool dimension='200' std_bitmap_path='bitmaps/standard' fix_bitmap_path='bitmaps/vario'>


<!-- Working Set -->
  <workingset name='imiIsoMaskupload' background_colour='white' selectable='yes' active_mask='MainMask'>
    <outputstring pos_x='0' pos_y='0' width='60' height='8'
      background_colour='white' font_attributes='FontAttributesSKM6x8'
      options='autowrap' value='IsoAgLib' />
    <outputstring pos_x='0' pos_y='8' width='60' height='16'
      background_colour='white' font_attributes='FontAttributesSKM6x8'
      options='autowrap' value='Tutorial 3.0' />

    <language code="en" />
    <language code="de" />
    <language code="it" />
  </workingset>


<!-- Attributes -->
  <!-- NOTE: With IsoAgLib's approach, you can exchange whole objects, which is helpful if not only the
       value should be exchanged, but also other atrtibutes or complete objects. See this example with the font_type and font_colour! -->
  <fontattributes name='FontAttributesNormal6x8' font_colour='black' font_size='6x8' font_type="latin1"/>

  <fontattributes name='FontAttributesNormal6x8LangDep' language="en" font_colour='black' font_size='6x8' font_type="latin1"/>
  <fontattributes name='FontAttributesNormal6x8LangDep' language="de" font_colour='red' font_size='6x8' font_type="latin9"/>
  <fontattributes name='FontAttributesNormal6x8LangDep' language="it" font_colour='blue' font_size='6x8' font_type="latin9"/>

  <fontattributes name='FontAttributesNormal8x12' font_colour='black' font_size='8x12' font_type="latin1"/>
  <fontattributes name='FontAttributesNormal6x8R' font_colour='red' font_size='6x8' font_type="latin1"/>
  <fontattributes name='FontAttributesNormal6x8B' font_colour='blue' font_size='6x8' font_type="latin1" font_style='bold+underlined+italic' />
  <fontattributes name='FontAttributesNormal8x8' font_colour='black' font_size='8x8' font_type="latin1" />
  <fontattributes name='FontAttributesNormal12x16' font_colour='black' font_size='12x16' font_type="latin1" />
  <fontattributes name='FontAttributesNormal16x16' font_colour='black' font_size='16x16' font_type="latin1" />
  <fontattributes name='FontAttributesNormal16x24' font_colour='black' font_size='16x24' font_type="latin1" />
  <fontattributes name='FontAttributesNormal24x32' font_colour='black' font_size='24x32' font_type="latin1" />
  <lineattributes name='LineAttributesNormal' line_colour='black' line_width='1' line_art='1111111111111111'/>
  <lineattributes name='LineAttributesDotted' line_colour='black' line_width='1' line_art='1001100110011001'/>

  <fontattributes name='FontAttributesSKM6x8' font_colour='black' font_size='6x8' font_type="latin1" />
  <fontattributes name='FontAttributesSKM8x8' font_colour='black' font_size='8x8' font_type="latin1" />
  <fontattributes name='FontAttributesSKM12x16' font_colour='black' font_size='12x16' font_type="latin1" />
  <lineattributes name='LineAttributesSKM' line_colour='black' line_width='1' line_art='1111111111111111'/>

  <!-- buttons: use separate font to allow different scaling apart from FontAttributesNormal6x8 -->
  <!--          button border is not rescaled => fonts inside buttons will have different scaling factor, depending on the available size -->
  <fontattributes name='FontAttributesNormal6x8_Button60x30' font_colour='black' font_size='6x8' font_type="latin1"/>

<!-- Variables -->

  <numbervariable name='ValSpeed' value='0' />
  <numbervariable name='ValAccel' value='10' />
  <numbervariable name='ValMiles' value='0' id='1234'/>

  <stringvariable name='StrNone' language="en" value='none selected!'       />
  <stringvariable name='StrNone' language="de" value='nichts ausgewählt'    />
  <stringvariable name='StrNone' language="it" value='niente selezionato!'  />

  <stringvariable name='StrRed' language="en" value="red" />
  <stringvariable name='StrRed' language="de" value="rot" />
  <stringvariable name='StrRed' language="it" value="rosso" />
  <stringvariable name='StrGreen' language="en+de+it" />
  <stringvariable name='StrBlue' language="*" />

  <stringvariable name='StrYellow' value='yellow' />
  <stringvariable name='StrCyan' value='cyan' />
  <stringvariable name='StrMagenta' value='magenta' />
  <stringvariable name='StrBlack' value='black' />
  <stringvariable name='StrWhite' value='white' />

  <outputstring name='LikeIt' value='That makes me happy.' width='200' height='8' font_attributes='FontAttributesNormal6x8' />
  <outputstring name='LikeItNot' value='What did I do wrong?' width='200' height='8' font_attributes='FontAttributesNormal6x8' />

  <container name='containerInAllMasks' width='200' height='60'>
    <rectangle pos_x='5' pos_y='5' width='190' height='15' line_attributes='LineAttributesDotted'/>



    <!-- file1='simpleBitmapManuellSchwarzWeiss' -->
    <!-- file4='' tells to disable 16 bit output! -->
    <picturegraphic name='BigLogo' pos_x='0' pos_y='0' width='120' transparency_colour='white' options='transparent+flashing'
                   format='8bit' rle='auto' file='simpleBitmap.bmp' file4='' >
      <fixedbitmap format='4bit' rle='auto' file='simpleBitmap240.bmp' file1='' />
    </picturegraphic>


    <picturegraphic pos_x='100' pos_y='10' width='64' 
                    transparency_colour='white' options='transparent+flashing'
                    format='8bit' rle='auto' file='simpleBitmap.bmp' file4='' >
      <fixedbitmap  format='4bit' rle='auto' file='simpleBitmap240.bmp' file1='' />
    </picturegraphic>
    <picturegraphic pos_x='100' pos_y='70' width='64' 
                    transparency_colour='white' options='transparent+flashing'
                    format='8bit' rle='auto' file='simpleBitmap.bmp' file4='' >
      <fixedbitmap  format='4bit' rle='auto' file='simpleBitmap240.bmp' file1='' />
    </picturegraphic>
    <!-- first try of a graphics context 
         Options: transparent , uselinefontfillcolourfordraw
      -->
    <graphicscontext name='BigGC'
                     viewport_width='128' viewport_height='128'
		     viewport_x='0' viewport_y='0'
		     canvas_width='256' canvas_height='256'
		     viewport_zoom='0'
		     foreground_colour='green' background_colour='red'
		     font_attributes='FontAttributesNormal24x32'
		     line_attributes='LineAttributesSKM'
		     fill_attributes='FillAttributes'
		     transparency_colour='red'
		     format='8bit'
		     options='transparent'
		     pos_x='100' pos_y='10' >
    </graphicscontext>
    <picturegraphic pos_x='100' pos_y='40' width='64' 
                    transparency_colour='white' options='transparent+flashing'
                    format='8bit' rle='auto' file='simpleBitmap.bmp' file4='' >
      <fixedbitmap  format='4bit' rle='auto' file='simpleBitmap240.bmp' file1='' />
    </picturegraphic>
    <picturegraphic pos_x='100' pos_y='100' width='64' 
                    transparency_colour='white' options='transparent+flashing'
                    format='8bit' rle='auto' file='simpleBitmap.bmp' file4='' >
      <fixedbitmap  format='4bit' rle='auto' file='simpleBitmap240.bmp' file1='' />
    </picturegraphic>


    <line name='leine' pos_x='0' pos_y='27' width='200' height='1' line_attributes='LineAttributesNormal' />
  </container>

  <key name='KeyReset' background_colour='white'>
    <outputstring name='resetTextOS' language="en+de+it" pos_x='0' pos_y='12' width='60' height='8' background_colour='white' font_attributes='FontAttributesSKM6x8' horizontal_justification='middle' />
  </key>

</objectpool>
